@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix voaf: <http://purl.org/vocommons/voaf#> .
@prefix dul: <http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix td: <http://www.w3.org/ns/td#> .

<http://www.w3.org/ns/wot/ssn> rdf:type owl:Ontology ;
  owl:imports <http://www.w3.org/ns/ssn/> ;
  owl:imports <http://www.w3.org/ns/td> .


td:Thing rdfs:subClassOf ssn:System .

td:providesInteractionPattern rdfs:subPropertyOf ssn:implements .

td:InteractionPattern rdfs:subClassOf sosa:Procedure .

td:hasInputData rdfs:subPropertyOf ssn:hasInput .

td:hasOutputData rdfs:subPropertyOf ssn:hasOutput .