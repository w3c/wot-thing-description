        sh:class td:MultiLanguage ;
        sh:order "3"^^xsd:integer ;
    ] ;
    sh:property [
        sh:path wotsec:proxy ;
        sh:datatype xsd:anyURI ;
        sh:order "4"^^xsd:integer ;
    ] .

:NoSecuritySchemeShape a sh:NodeShape ;
