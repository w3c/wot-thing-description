{
  "@context": {
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "wotsec": "http://www.w3.org/ns/wot-security#",
    "in": {
      "@id": "http://www.w3.org/ns/wot-security#in"
    },
    "authorization": {
      "@id": "http://www.w3.org/ns/wot-security#authorization"
    },
    "token": {
      "@id": "http://www.w3.org/ns/wot-security#token"
    },
    "refresh": {
      "@id": "http://www.w3.org/ns/wot-security#refresh"
    },
    "proxy": {
      "@id": "http://www.w3.org/ns/wot-security#proxy"
    },
    "scopes": {
      "@id": "http://www.w3.org/ns/wot-security#scopes",
      "@container": "@set"
    },
    "flow": {
      "@id": "http://www.w3.org/ns/wot-security#flow"
    },
    "qop": {
      "@id": "http://www.w3.org/ns/wot-security#qop"
    },
    "alg": {
      "@id": "http://www.w3.org/ns/wot-security#alg"
    },
    "format": {
      "@id": "http://www.w3.org/ns/wot-security#format"
    },
    "identity": {
      "@id": "http://www.w3.org/ns/wot-security#identity"
    },
    "scheme": {
      "@id": "rdf:type",
      "@type": "@vocab"
    },
    "nosec": "wotsec:NoSecurityScheme",
    "basic": "wotsec:BasicSecurityScheme",
    "digest": "wotsec:DigestSecurityScheme",
    "apikey": "wotsec:APIKeySecurityScheme",
    "bearer": "wotsec:BearerSecurityScheme",
    "cert": "wotsec:CertSecurityScheme",
    "psk": "wotsec:PSKSecurityScheme",
    "public": "wotsec:PublicSecurityScheme",
    "pop": "wotsec:PoPSecurityScheme",
    "oauth2": "wotsec:OAuth2SecurityScheme"
  }
}
