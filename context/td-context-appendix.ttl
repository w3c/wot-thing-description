@prefix ld: <http://www.w3.org/ns/json-ld#> .
@prefix td: <http://www.W3.org/ns/td#> .
@prefix jsonschema: <http://www.W3.org/ns/json-schema#> .
@prefix wotsec: <http://www.W3.org/ns/wot-security#> .

_:Thing a ld:Mapping ;
        ld:iri td:Thing ;
        ld:context <http://www.w3.org/ns/td> .

_:DataSchema a ld:Mapping ;
        ld:iri jsonschema:DataSchema ;
        ld:context <http://www.w3.org/ns/json-schema> .

_:SecurityScheme a ld:Mapping ;
        ld:iri wotsec:SecurityScheme ;
        ld:context <http://www.w3.org/ns/wot-security> .
